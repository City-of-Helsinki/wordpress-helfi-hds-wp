{
    "apiVersion": 2,
    "title": "Helsinki - Accordion",
    "name": "hds-wp/accordion",
    "icon": "format-gallery",
    "textdomain": "hds-wp",
    "category": "hds-wp",
    "supports": {
      "anchor": true
    },
    "attributes": {
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "panels": {
            "type": "array",
            "default": []
        },
        "blockVersion": {
            "type": "integer"
        },
        "anchor": {
            "type": "string",
            "default": ""
        },
        "preview": {
            "type": "string",
            "default": ""
        }
    },
    "render_callback": "hds_wp_render_block_accordion"
}
